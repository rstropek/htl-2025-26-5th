{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "start:darwin:linux": "node build/set-env.js && npm run generate-web-api && ng serve --port $PORT",
    "start:win32": "node build/set-env.js && npm run generate-web-api && ng serve --port %PORT%",
    "build": "ng build",
    "test": "ng test",
    "generate-web-api": "npx ng-openapi-gen --input WebApi.json --output src/app/api"
  },
  "prettier": {
    "printWidth": 150,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/platform-browser": "^21.0.3",
    "@angular/router": "^21.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "^5.13.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-openapi-gen": "^1.0.4",
    "run-script-os": "^1.1.6",
    "typescript": "~5.9.2"
  }
}
